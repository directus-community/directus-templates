      }
    },
    "fonts": {
      "display": {
        "fontFamily": "\"Space Grotesk\", system-ui",
        "fontWeight": "600"
      },
      "sans": {
        "fontFamily": "\"DM Sans\", system-ui"
      }
    },
    "borderRadius": "6px",
    "sidebar": {
      "background": "#FFFFFF",
      "borderWidth": "1px",
      "borderColor": "var(--theme--border-color-subdued)",
      "section": {
        "toggle": {
          "borderColor": "var(--theme--border-color-subdued)",
